<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BaseCircleProgress">
        <attr name="progressWidth" format="dimension"/>
        <attr name="emptyColor" format="color"/>
        <attr name="startAngle" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CircleProgress">
        <attr name="progress" format="integer"/>
        <attr name="progressColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="MultiCircleProgress">
        <attr name="progress1" format="integer"/>
        <attr name="progressColor1" format="color"/>
        <attr name="progress2" format="integer"/>
        <attr name="progressColor2" format="color"/>
        <attr name="progress3" format="integer"/>
        <attr name="progressColor3" format="color"/>
    </declare-styleable>

</resources>